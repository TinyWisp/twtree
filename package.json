{
  "name": "twtree",
  "version": "0.1.2",
  "description": "A highly customizable tree component for vue",
  "main": "lib/twtree.umd.min.js",
  "keyword": ["vue", "tree", "javascript"],
  "author": "yinquan",
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/TinyWisp/twtree",
  "scripts": {
    "serve": "vue-cli-service serve",
